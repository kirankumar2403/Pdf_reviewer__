{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/health",
      "dest": "/api/index"
    },
    {
      "src": "/upload/(.*)",
      "dest": "/api/index"
    },
    {
      "src": "/extract/(.*)",
      "dest": "/api/index"
    },
    {
      "src": "/invoices/(.*)",
      "dest": "/api/index"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ]
}